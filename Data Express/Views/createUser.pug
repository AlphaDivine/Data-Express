doctype
html
    head
        link(rel = 'stylesheet', href = "style.css")
        link(href="data:image/x-icon;base64," rel="icon" type="image/x-icon")
        title Data Express
    body
        h1 Welcome to the Create A User Page!
        h2 Please fill out all the spaces
        form(method='', action='')
            label(for='username') Username:
            br
            input(name='username', id='name',)
            p
            label(for='password') Password:
            br
            input(name='password', id='pass', type='password',)
            p
            label(for='email') Email:
            br
            input(name='email', id='email')
            p
            label(for='age') Age:
            br
            input(name='age', id='age')
            p
            label(for='question1') What is your favorite animal? :
            br
            select(class='choose',id='question1')
                option(value=0) Panda
                option(value=1) Lion
                option(value=2) Chicken
                option(value=3) Monkey
            p
            label(for='question2') What is your favorite sport? :
            br
            select(class='choose',id='question2')
                option(value=0) Soccer
                option(value=1) Baseball
                option(value=2) Football
                option(value=3) Ultimate Frisbee
            p
            label(for='question3') What is your favorite video game genre
            br
            select(class='choose',id='question3')
                option(value=0) First Person Shooter
                option(value=1) RPG
                option(value=2) Strategy
                option(value=3) Sports
            p
            a(class='button',id = 'submitnewuser', href='/User') Submit


